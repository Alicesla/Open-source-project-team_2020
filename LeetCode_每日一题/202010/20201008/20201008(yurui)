class Solution {
public:
    void reverseString(vector<char>& s) {
        int n=s.size();
        int left=0,right=n-1;
        int temp;
        while(left<right)
        {
           temp=s[left];
           s[left]=s[right];
           s[right]=temp;
           left++;
           right--;
        }
        
    }
};
