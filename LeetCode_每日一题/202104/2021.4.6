class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
       for(int i=m,j=0;i<m+n;i++,j++)
       {
           nums1[i]=nums2[j];
       }
       //sort(nums1.begin(),nums1.end());
       for(int i=m;i<m+n;i++)
       {   
           int temp=nums1[i];
           for(int j=0;j<i;j++)
           {
               if(nums1[j]>nums1[i])
               {
                   for(int k=i-1;k>=j;k--)
                   {
                       nums1[k+1]=nums1[k];
                   }
                   nums1[j]=temp;
               }
           }
       }
    }
};
