/**
 * Definition for a binary tree node.
 * struct TreeNode {
 *     int val;
 *     TreeNode *left;
 *     TreeNode *right;
 *     TreeNode(int x) : val(x), left(NULL), right(NULL) {}
 * };
 */
class Solution {
    int count=0;
public:
    int countNodes(TreeNode* root) {
           if(root==NULL) return 0;
           else count++;
           countNodes(root->left);
           countNodes(root->right); 
            return count;     
    }
};
